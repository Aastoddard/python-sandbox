{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Fake Branch Return",
    "description": "A basic Jenkins return for the branches in a repo",
    "type": "object",
    "properties":{
        "branches": {
            "description": "The branch list",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "branchName": {
                        "type": "string",
                        "minLength": 5,
                        "maxLength": 12
                    }
                },
                "required": [
                    "branchName"
                ]
            }
        },
        "buildable":{
            "description": "Whether the branch is buildable or not",
            "type": "boolean"
        }
    },
    "required": [
        "branches",
        "buildable"
    ]
}